#, c-format
msgid "macopix COMMON DIR:%s\n"
msgstr "ТИПОВА ТЕКА macopix:%s\n"

#: src/main.c:4890
msgid " macopix : Mascot Constructive Pilot for X   Ver"
msgstr ""
" macopix: Інструмент конструювання маскотів для віконних менеджерів Вер."

#: src/main.c:4891
#, c-format
msgid "  [usage] %% macopix [options] (mascot file)\n"
msgstr "  [використання] %% macopix [параметри] (файл маскота)\n"

#: src/main.c:4892
msgid "     -h, --help               : Print this message\n"
msgstr "     -h, --help              : Надрукувати це повідомлення\n"

#: src/main.c:4894
msgid "     -c, --common-dir         : Print COMMON Directory\n"
msgstr "     -c, --common-dir        : Надрукувати ТИПОВУ теку\n"

#: src/main.c:4897
msgid "     -b, --biff               : Turn on the biff function\n"
msgstr ""
"     -b, --biff              : Увімкнути функцію повідомлення про листи\n"

#: src/main.c:4899
msgid "     -s, --signal             : Turn on the time signal\n"
msgstr "     -s, --signal            : Увімкнути сигнал годинника\n"

#: src/main.c:4900
#, c-format
msgid "     -o, --offset offset[%%]   : Set offset-value for Focus Following\n"
msgstr ""
"     -o, --offset offset[%%]  : Встановити відступ для відслідковування "
"фокусу\n"

#: src/main.c:4901
msgid "     -r, --rcfile rc-file     : Set the resource file\n"
msgstr "     -r, --rcfile rc-file    : Встановити файл ресурсу\n"

#: src/main.c:4902
msgid "     -m, --menu   menu-file   : Set the file for quick menu\n"
msgstr "     -m, --menu   menu-file  : Встановити файл для швидкого меню\n"

#: src/main.c:4903
msgid ""
"     -mg --mag    magnify     : Set magnification percentage of mascot\n"
msgstr ""
"     -mg --mag    magnify    : Встановити процент збільшення для маскота\n"

#: src/main.c:4904
msgid ""
"     -i, --ip-style (0|1|2|3) : Set interpolation style for magnification\n"
msgstr ""
"     -i, --ip-style (0|1|2|3): Встановити тип інтерполяції для збільшення\n"

#: src/main.c:4905
msgid "## For Installation\n"
msgstr "## Для встановлення\n"

#: src/main.c:4906
msgid "     -a, --auto-install       : Set mascot auto install ON\n"
msgstr "     -a, --auto-install      : Увімкнути автовстановлення маскотів\n"

#: src/main.c:4907
msgid "     -na --no-auto-install    : Set mascot auto install OFF\n"
msgstr "     -na --no-auto-install   : Вимкнути автовстановлення маскотів\n"

#: src/main.c:4908
msgid "     -O, --over-write         : Permit to overwrite files in user dir\n"
msgstr ""
"     -O, --over-write        : Дозволити перезапис файлів у теці "
"користувачів\n"

#: src/main.c:4909
msgid ""
"     --install-user-mascot (mascot)  : Mascot Installation to User Dir.\n"
msgstr ""
"     --install-user-mascot (mascot) : Встановлення маскота у теку "
"користувача.\n"

#: src/main.c:4910
msgid "     --install-user-menu (menu)      : Menu Installation to User Dir.\n"
msgstr ""
"     --install-user-menu (menu)     : Встановлення меню у теку "
"користувача.\n"

#: src/main.c:4912
msgid ""
"     --install-common-mascot (mascot): Mascot Installation to Common Dir.\n"
msgstr ""
"     --install-common-mascot (mascot): Встановлення маскота у типову теку.\n"

#: src/main.c:4913
msgid ""
"     --install-common-menu (menu)    : Menu Installation to Common Dir.\n"
msgstr ""
"     --install-common-menu (menu)   : Встановлення меню у типову теку.\n"

#: src/main.c:4916
msgid "## For Socket Message\n"
msgstr "## Для повідомлень сокетів\n"

#: src/main.c:4917
msgid ""
"     --sockmsg                : Start MaCoPiX as Socket Message Receiver\n"
msgstr ""
"     --sockmsg               : Запустити MaCoPiX як отримувач повідомлень "
"сокетів\n"

#: src/main.c:4918
msgid ""
"     --message (text)         : Send Message to Socket Message Receiver\n"
msgstr ""
"     --message (text)        : Надіслати повідомлення до отримувача "
"повідомлень сокета\n"

#: src/main.c:4919
msgid "     --message-expire (time)  : Socket Message Expire Time[ms]\n"
msgstr ""
"     --message-expire (time) : Час застарівання повідомлення сокета[мс]\n"

#: src/main.c:4920
msgid "     --message-type-lump      : Force to Set Lump Sum Balloon\n"
msgstr ""
"     --message-type-lump     : Силоміць встановити показу сукупного "
"повідомлення\n"

#: src/main.c:4921
msgid "     --message-type-step      : Force to Set Stepping Balloon\n"
msgstr ""
"     --message-type-step     : Силоміць встановити покроковий показ "
"повідомлень\n"

#: src/main.c:4922
msgid "     --message-step (frame)   : Stepping Interval Frame for SockeMsg\n"
msgstr ""
"     --message-step (кадри)  : Величина кроку у кадрах для повідомлень "
"сокета\n"

#: src/main.c:4923
msgid "     -nc, --no-duet-click     : Duet Animation [Click] OFF\n"
msgstr "     -nc, --no-duet-click    : Вимкнути анімацію дуета [Клацання]\n"

#: src/main.c:4924
msgid "     -nr, --no-duet-random    : Duet Animation [Random] OFF\n"
msgstr "     -nr, --no-duet-random   : Вимкнути анімацію дуету [Випадкові]\n"

